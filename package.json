{
  "name": "vue-caly",
  "version": "0.0.1",
  "main": "dist/vue-caly-lib.min.mjs",
  "files": [
    "dist/*"
  ],
  "license": "MIT",
  "private": false,
  "author": {
    "name": "Paul Spenke",
    "email": "info@onlineer.de"
  },
  "repository": "https://github.com/deepDiverPaul/vue-caly",
  "bugs": "https://github.com/deepDiverPaul/vue-caly/issues",
  "keywords": [
    "vue",
    "calendly",
    "cal.com",
    "datepicker"
  ],
  "scripts": {
    "dev": "vite -c vite-app.config.ts",
    "build:app": "run-p type-check build-app",
    "build": "run-p build-lib",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "test:e2e": "playwright test",
    "build-app": "vite build --outDir dist-app -c vite-app.config.ts",
    "build-lib": "vite build --outDir dist --sourcemap true",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "luxon": "^3.1.1",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@playwright/test": "^1.28.1",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.1",
    "@types/luxon": "^3.1.0",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.20",
    "sass": "^1.57.0",
    "tailwindcss": "^3.2.4",
    "typescript": "~4.7.4",
    "vite": "^4.0.0",
    "vitest": "^0.25.6",
    "vue-tsc": "^1.0.12"
  }
}
